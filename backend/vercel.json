{
  "version": 2,
  "builds": [
    {
      "src": "server.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.py"
    }
  ],
  "env": {
    "MONGO_URL": "@mongo_url",
    "DB_NAME": "@db_name",
    "JWT_SECRET": "@jwt_secret",
    "STRIPE_API_KEY": "@stripe_api_key",
    "CORS_ORIGINS": "@cors_origins"
  }
}
